<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wiatec.blive.orm.dao.CoinVoucherDao">

    <select id="selectAll" resultType="com.wiatec.blive.orm.pojo.CoinVoucherInfo">
        SELECT * FROM bvision_coin_voucher
    </select>

    <select id="selectOnyByVoucherId" resultType="com.wiatec.blive.orm.pojo.CoinVoucherInfo">
      SELECT * FROM bvision_coin_voucher WHERE voucher_id = #{voucherId}
    </select>

</mapper>