<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wiatec.blive.orm.dao.UpgradeDao">

    <select id="selectOne" resultType="com.wiatec.blive.orm.pojo.UpgradeInfo">
        SELECT id, name, url, version, code, info
        FROM upgrade
        WHERE id =1
    </select>

</mapper>